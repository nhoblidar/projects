      "source": [
        "from sklearn.metrics import classification_report\n",
        "from sklearn.preprocessing import StandardScaler\n",
        "\n",
        "scaler = StandardScaler()\n",
        "\n",
        "def production(X_path, y_path):\n",
        "\n",
        "    model = TestModel()\n",
        "\n",
        "    df_X = pd.read_csv(X_path)\n",
        "\n",
        "    df_X['Distance'] = df_X['Distance'].str.extract(r'(\\d+)').astype(float).astype('Int64')\n",
        "\n",
        "    for col in ['PreviousSalary', 'Salary']:\n",
        "      df_X[col] = df_X[col].str.replace('K', '', regex=False).astype(float) * 1000\n",
        "      df_X[col] = df_X[col].astype('Int64')\n",
        "\n",
        "    float_cols_to_int = ['AttendanceRate', 'SelfReview', 'SupervisorReview']\n",
        "    df_X[float_cols_to_int] = df_X[float_cols_to_int].round().astype('Int64')\n",
        "    df_X['ReviewDiff'] =  df_X['SupervisorReview']-df_X['SelfReview']\n",
        "    df_X['SalaryDiff'] = df_X['Salary'] - df_X['PreviousSalary']\n",
        "\n",
        "    numeric_features = ['Gender', 'Distance', 'StressLevel', 'DepartmentCode', 'Salary', 'PreviousSalary','SelfReview','SupervisorReview','SalaryDiff', 'ReviewDiff','YearsWorked']\n",
        "\n",
        "    X = df_X[numeric_features]\n",
        "    X_scaled = scaler.fit_transform(X)\n",
        "\n",
        "    pred = model.predict(X_scaled)\n",
        "\n",
        "    df_y = pd.read_csv(y_path)['Left']\n",
        "    print(classification_report(df_y, pred))\n",
        "\n",
        "\n",
        "production(\n",
        "    X_path='https://raw.githubusercontent.com/msaricaumbc/DS_data/master/ds602/final/employee_departure_dataset_X.csv',\n",
        "    y_path='https://raw.githubusercontent.com/msaricaumbc/DS_data/master/ds602/final/employee_departure_dataset_y.csv'\n",
        ")\n"
      ]
    }
  ]
