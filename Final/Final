        "import joblib\n",
        "import pandas as pd\n",
        "\n",
        "class TestModel:\n",
        "    def __init__(self):\n",
        "        self.model = joblib.load('dtmodel.pkl')\n",
        "\n",
        "    def predict(self, X):\n",
        "        return self.model.predict(X)\n"
      ],
